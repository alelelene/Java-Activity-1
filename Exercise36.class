import java.util.Scanner;
public class Exercise36
{
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("Initial number of stars: ");
		int num = scanner.nextInt();
		
		while(num>0) {
		    
		    for(int i = 1; i <= num; i++) {
		        System.out.print("*");
		    }
		    
		    System.out.println();
		    num--;
		    
		}
		
	
	}
}
